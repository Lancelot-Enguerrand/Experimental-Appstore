{
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "services": [
    {
      "name": "ctfreak",
      "image": "jypsoftware/ctfreak:1.37.0",
      "isMain": true,
      "environment": [
          { "key": "TZ", "value": "${TZ}" }
      ],
      "internalPort": 6700,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/data",
          "containerPath": "/data"
        }
      ]
    }
  ]
}