{
  "schemaVersion": 2,
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "services": [
    {
      "name": "tasktrove",
      "image": "ghcr.io/dohsimpson/tasktrove:v0.12.4",
      "isMain": true,
      "internalPort": 3000,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/app",
          "containerPath": "/app/data"
        }
      ]
    }
  ]
}

